            steps {
                echo "Hello deploy process!!"
            }
        }
    },
    stages {
        stage('Test') {
            steps {
                echo "Hello Test Process!!"
            }
        }
    },
    stages {
        stage('Relese') {
            steps {
                echo "Hello Relese Process!!"
            }
        }
    }
}